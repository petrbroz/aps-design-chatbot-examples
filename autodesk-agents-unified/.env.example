# Environment Variables for AgentCore System
# Copy this file to .env and fill in your actual values

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Autodesk API Configuration
AUTODESK_CLIENT_ID=your_autodesk_client_id
AUTODESK_CLIENT_SECRET=your_autodesk_client_secret
AUTODESK_ACCESS_TOKEN=your_autodesk_access_token

# OpenSearch Configuration (optional - defaults to localhost)
OPENSEARCH_ENDPOINT=http://localhost:9200
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=admin

# Development Settings
DEVELOPMENT=true
LOG_LEVEL=DEBUG
CACHE_DIRECTORY=./dev_cache

# Optional: Disable authentication for local development
AUTH_ENABLED=false